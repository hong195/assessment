<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Users" table="users">
    <id name="idId" type="guid" column="id_id" length="36">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nameFirstName" type="string" column="name_first_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nameLastName" type="string" column="name_last_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namePatronymic" type="string" column="name_patronymic" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="loginLogin" type="string" column="login_login" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roleRole" type="string" column="role_role" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
